{
	"name": "DS_destination",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_Blob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Schemaname": {
				"type": "string"
			},
			"Tablename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().Schemaname,'.',dataset().Tablename)",
					"type": "Expression"
				},
				"container": "destination"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Name",
				"type": "String"
			},
			{
				"name": "Category",
				"type": "String"
			},
			{
				"name": "Status",
				"type": "String"
			},
			{
				"name": "Error",
				"type": "String"
			}
		]
	}
}